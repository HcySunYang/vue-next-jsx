{
  "name": "@hcysunyang/babel-plugin-vue-next-jsx",
  "version": "0.2.0",
  "main": "dist/main.js",
  "repository": "https://github.com/HcySunYang/vue-next-jsx.git",
  "author": "HcySunYang <HcySunYang@outlook.com>",
  "license": "MIT",
  "scripts": {
    "dev": "TARGET=playground rollup -cw",
    "build": "rollup -c",
    "test": "jest",
    "commit": "git-cz",
    "lint": "prettier --write --parser typescript \"src/**/*.ts?(x)\"",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepublish": "npm run build && npm run test"
  },
  "dependencies": {
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/types": "^7.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/traverse": "^7.10.3",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/babel-types": "^7.0.7",
    "@types/babel__core": "^7.1.9",
    "@types/babel__traverse": "^7.0.12",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "monaco-editor": "^0.20.0",
    "prettier": "^2.0.5",
    "rollup": "^2.18.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5",
    "vue": "^3.0.0-beta.20"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
